<h3>
<ol>

  <p> in properties/_form.erb <p>

<%= form_for @property do |f| %>

    <%= f.label :name, "Property Name" %>
    <%= f.text_field :name %> <br>
    <%= f.label :address, "Address" %>
    <%= f.text_field :address %> <br>
    <%= f.label :zip_code, "Zip Code" %>
    <%= f.text_field :zip_code %> <br>
    <%= f.label :pool_size, "Pool Size(sq ft)" %>
    <%= f.text_field :pool_size %> <br>
    <%= f.label :pool_material, "pool_material" %>
    <%= f.text_field :pool_material %> <br>
    <%= f.label :pool_cover, "Job Description" %>
    <%= f.text_field :pool_cover %> <br>
    <%= f.hidden_field :client_id, value: session[:user_id] %>
    <%= f.submit %>
<% end %>
</ol>
