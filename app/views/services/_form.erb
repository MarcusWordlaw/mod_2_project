<ol>
<%= form_for @service do |f| %>
    <%= f.label :property_id  %>
    <%= f.collection_select(:property_id, Property.all, :id, :name) %> <br>
    <%= f.label :employee_id  %>
    <%= f.collection_select(:employee_id, Employee.all, :id, :name) %> <br>
    <%= f.label :appointment, "Appointment" %>
    <%= f.datetime_local_field :appointment %> <br>
    <%= f.label :service_cost, "Labor Cost" %>
    <%= f.text_area :service_cost %> <br>
    <%= f.label :materials_cost, "Material Cost" %>
    <%= f.text_area :materials_cost %> <br>
    <%= f.label :total, "Total Cost" %>
    <%= f.text_area :total %> <br>
    <%= f.label :paid, "Paid" %>
    <%= f.text_area :paid %> <br>
    <%= f.label :job_description, "Job Description" %>
    <%= f.text_area :job_description %> <br>
    
    <%= f.submit %>
<% end %>
</ol>
